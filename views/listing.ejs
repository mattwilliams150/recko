                            <input type="checkbox" class="tagcheckbox" id="<%=tagId%>" name="<%=tagId%>" ><div class="mr-1 badge badge-pill tag-pill px-3 py-2 mb-2 tag-pill-button"><%=tagReadable%></div>
                        </label>
                    <% } %>
                      </div>

                    <button class="btn btn-primary" type="submit">Post review</button>
                  </form>
                </div>
              </div>
            </div>


