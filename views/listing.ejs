<% layout('layout') %>

 <script>var mongoplace = <%- JSON.stringify(mongoplace) %></script>
 <script>var reviews = <%- JSON.stringify(reviews) %></script>
 <script>var place = <%- JSON.stringify(place) %></script>
 <script>var categories = <%- JSON.stringify(categories) %></script>
 <script>var toptags = <%- JSON.stringify(toptags) %></script>


    <!-- Hero Section-->
    <section class="pt-7 pb-5 d-flex align-items-end dark-overlay bg-cover" style="background-image: url('../img/place_photos/<%= mongoplace.place_id %>.png');">
      <div class="container overlay-content">
        <div class="d-flex justify-content-between align-items-start flex-column flex-lg-row align-items-lg-end">
          <div class="text-white mb-4 mb-lg-0">
            <div class="badge badge-pill badge-transparent px-3 py-2 mb-4"><%=place[0].subcategory%></div>
            <h1 class="text-shadow verified"><%= mongoplace.name %></h1>
            <p><i class="fa-map-marker-alt fas mr-2"></i> <%= mongoplace.formatted_address %></p>
            <p class="d-flex align-items-center">

                <% for (j = 0; j < 5; j++) {
                    if (Math.round(mongoplace.rating) > j) {  %>
                        <% if (j == 4 ) { %>
                            <i class="fa fa-xs fa-star text-primary mr-2"></i>
                        <% } else { %>
                            <i class="fa fa-xs fa-star text-primary"></i>
                        <% } %>
                    <% } else { %>
                        <% if (j == 4 ) { %>
                            <i class="fa fa-xs fa-star text-gray-200 mr-2"></i>
                        <% } else { %>
                            <i class="fa fa-xs fa-star text-gray-200"></i>
                        <% } %>
                    <% } %>
                <% } %>
                <span class="text-white text-s mr-4"><%= mongoplace.rating %></span>
                <% if(relevanceAvailable) { %>
                <span class="lead"><span class="badge badge-secondary"><%=relevance%>%</span>  Match for you</span>
                <% } %>
              </p>
              <p class="mb-0" >
                <div class="form-group tag-pill-button">
                <% 
                     for (t=0; t<3; t++) {
                     var toptag = toptags[t]
                     var tagId = toptag[0]
                     var tagcount = toptag[1]
                     var tagReadable = categories.tagObj[tagId].name
                     var tagImage = categories.tagObj[tagId].image
                     if (tagcount > 0) {
                     %>
                        <div class="mr-1 badge badge-pill tag-pill px-3 py-2 mb-2 tag-pill-button tag-pill-large">
                            <img class="pr-2" style="width: 32px" src="img/icons/<%= tagImage %>" alt="<%=tagReadable%> Icon">
                            <%=tagReadable%> (<%=tagcount%>)
                        </div>
                     <%
                    }
                  }%>
                </div>
              </p>
          </div>
          <div class="calltoactions"><a class="btn btn-primary" href="#leaveReview" onclick="$('#leaveReview').collapse('show')" data-smooth-scroll>Leave a Review</a></div>
        </div>
      </div>
    </section>
    <section class="py-6">
      <div class="container">
        <div class="row">
          <div class="col-lg-8">
                        <div class="text-block">
              <p class="subtitle text-sm text-primary">Reviews    </p>
              <h5 class="mb-4">Listing Reviews </h5>

            <!-- Recko Reviews--->
            <% for (i = 0; i < reviews.length; i++) { %>
             <div class="media d-block d-sm-flex review">
                <div class="text-md-center mr-4 mr-xl-5"><img class="d-block avatar avatar-xl p-2 mb-2" src="img/letters/<%= reviews[i].name.charAt(0) %>.png" alt="Reviewer Photo"><span class="text-uppercase text-muted text-sm">

                    <% let datediff = Math.floor((Date.now() - reviews[i].date) / (1000*60*60*24))
                        let monthsago = Math.floor((Date.now() - reviews[i].date) / (1000*60*60*24*30)) %>

                        <% if(datediff == 0) { %>
                             Today
                        <% } else if (datediff < 7) { %>
                             This Week
                        <% } else if (datediff < 30) { %>
                             This Month
                        <% } else { %>
                             <%=monthsago%> Months Ago
                         <% } %>

                </span></div>
                <div class="media-body">
                  <h6 class="mt-2 mb-1"><%= reviews[i].name %></h6>
                  <div class="mb-2">
                    <% for (j = 0; j < 5; j++) { %>
                             <%  if (reviews[i].rating > j) {  %>
                                <i class="fa fa-xs fa-star text-primary"></i>
                            <% } else { %>
                                <i class="fa fa-xs fa-star text-gray-300"></i>
                            <% } %>
                        <% } %>
                  </div>
                  <p class="text-muted text-sm"><%= reviews[i].review %></p>
                  <p>
                    <% for (k=0; k<categories.tags.length; k++) { %>
                        <% var tagReadable = categories.tags[k] %>
                        <% var tagId = categories.tagIds[categories.tags[k]] %>

                        <% if(reviews[i][tagId] !== undefined) { %>
                            <div class="mr-1 badge badge-pill tag-pill px-3 py-2 mb-2 tag-pill-button"><%=tagReadable%></div>
                        <% } %>
                    <% } %>
                    </p>
                </div>


              </div>
            <% } %>

            <!-- Google Reviews--->
            <% for (i = 0; i < mongoplace.reviews.length; i++) { %>
              <div class="media d-block d-sm-flex review">
                <div class="text-md-center mr-4 mr-xl-5"><img class="d-block avatar avatar-xl p-2 mb-2" src="<%= mongoplace.reviews[i].profile_photo_url %>" alt="Reviewer Photo"><span class="text-uppercase text-muted text-sm"><%= mongoplace.reviews[i].relative_time_description %></span></div>
                <div class="media-body">
                  <h6 class="mt-2 mb-1"><%= mongoplace.reviews[i].author_name %></h6>
                  <div class="mb-2">
                    <%      
                        for (j = 0; j < 5; j++) {
                            if (Math.round(mongoplace.reviews[i].rating) > j) {  %>
                                <i class="fa fa-xs fa-star text-primary"></i>
                            <% } else { %>
                                <i class="fa fa-xs fa-star text-gray-300"></i>
                            <% } %>     
                        <% } %>
                  </div>
                  <p class="text-muted text-sm"><%= mongoplace.reviews[i].text %></p>
                </div>
              </div>
            <% } %>    
                
              <div class="py-5">
                <button class="btn btn-outline-primary" type="button" data-toggle="collapse" data-target="#leaveReview" aria-expanded="false" aria-controls="leaveReview">Leave a review</button>
                <div class="collapse mt-4" id="leaveReview">
                  <h5 class="mb-4">Leave a review</h5>
                  <form class="form" id="contact-form" method="post" action="/listingreview?placeid=<%= placeid %>">
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="form-group">
                          <label class="form-label" for="name">Your name *</label>
                          <input class="form-control" type="text" name="review_name" id="name" placeholder="Enter your name" required="required">
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="form-group">
                          <label class="form-label" for="rating">Your rating *</label>
                          <select class="custom-select focus-shadow-0" name="review_rating" id="rating">
                            <option value="5">&#9733;&#9733;&#9733;&#9733;&#9733; (5/5)</option>
                            <option value="4">&#9733;&#9733;&#9733;&#9733;&#9734; (4/5)</option>
                            <option value="3">&#9733;&#9733;&#9733;&#9734;&#9734; (3/5)</option>
                            <option value="2">&#9733;&#9733;&#9734;&#9734;&#9734; (2/5)</option>
                            <option value="1">&#9733;&#9734;&#9734;&#9734;&#9734; (1/5)</option>
                          </select>
                        </div>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="form-label" for="email">Your email *</label>
                      <input class="form-control" type="email" name="review_email" id="email" placeholder="Enter your  email" required="required">
                    </div>
                    <div class="form-group">
                      <label class="form-label" for="review">Review text *</label>
                      <textarea class="form-control" rows="4" name="review_review" id="review" placeholder="Enter your review" required="required"></textarea>
                    </div>
                      <div class="form-group"><label class="form-label" for="review">Select any tags for this place</label>   </div>



                      <div class="form-group tag-pill-button">

                    <% for (i=0; i<categories.tags.length; i++) { %>
                    <% var tagReadable = categories.tags[i] %>
                    <% var tagId = categories.tagIds[categories.tags[i]] %>
